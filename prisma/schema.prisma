// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Payment {
  id Int @id @default(autoincrement())
  uuid String @unique
  orderId String
  status String
  amount String
  paymentAmount String?
  isFinal Boolean
  url String
  chatId Int
}